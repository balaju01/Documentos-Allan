\pard \ltrpar \ql\li0\ri0\sb60\sa120\nowidctlpar\wrapdefault\faauto\rin0\lin0\itap0 \plain \rtlch\fcs1\ltrch\fcs0\f2\fs24\lang1033\cgrid0 
