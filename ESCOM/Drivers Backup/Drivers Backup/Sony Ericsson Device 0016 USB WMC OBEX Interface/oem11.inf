Class=Ports
ClassGuid={4D36E978-E325-11CE-BFC1-08002BE10318}
Provider=%Provider%
DriverVer=04/24/2008,4.44.2.0
CatalogFile=s0016obex.cat

[ControlFlags]
ExcludeFromSelect = *

[SourceDisksNames.x86]
; diskid = description[, [tagfile] [, <unused>, subdir]]
1=%DiskString%,"s0016obex.sys",,\i386

[SourceDisksNames.amd64]
; diskid = description[, [tagfile] [, <unused>, subdir]]
1=%DiskString%,"s0016obex.sys",,\amd64


[SourceDisksFiles]
; filename = diskid[,[ subdir][, size]]
s0016obex.sys = 1
s0016cmnt.sys = 1

