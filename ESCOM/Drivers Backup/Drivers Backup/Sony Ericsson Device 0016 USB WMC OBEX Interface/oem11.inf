;-----------------------------------------------------------------------------
; Installation instructions
;-----------------------------------------------------------------------------

[sed0016.Install.nt]
AddReg		= sed0016.AddReg,sed0016.NT.AddReg
CopyFiles	= CopyFiles, CopyFiles.Win2K


;-----------------------------------------------------------------------------
;  Registry
;-----------------------------------------------------------------------------

; common registry entries 
[sed0016.AddReg]
HKR,,NTMPDriver,,s0016obex.sys
HKR,,OverrideDefaultPortSettings, 0x10001, 01,00,00,00
HKR,,InitialBaudRate, 0x10001, 00,C2,01,00			;115200``nitial baud rate
HKR,,InitialLineControl,, "8N1"					;8-bits, No parity, 1 stop bit

; only for NT...
[sed0016.NT.AddReg]
HKR,,PortSubClass,1,01
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

;-----------------------------------------------------------------------------
;  Copy lists
;-----------------------------------------------------------------------------
[CopyFiles]

[CopyFiles.Win2K]
s0016obex.sys
s0016cm.sys,s0016cmnt.sys
s0016cmnt.sys



;-----------------------------------------------------------------------------
;  NT Services installation
;-----------------------------------------------------------------------------



[sed0016.Install.NT.Services]
AddService = s0016obex, 0x00000002, sed0016.Install.AddService




[sed0016.Install.AddService]
DisplayName	= %sed0016.Service.Name%
Description	= %sed0016.Service.Desc%
ServiceType	= 1			;%SERVICE_KERNEL_DRIVER%
StartType	= 3			;%SERVICE_DEMAND_START%
ErrorControl	= 1			;%SERVICE_ERROR_NORMAL%	
ServiceBinary	= %12%\s0016obex.sys



